generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Schedule {
  id           String   @id @default(uuid())
  customerName String
  time         DateTime @db.Timestamp(6)
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@map("schedules")
}
